    <link rel="stylesheet" href="/css/recContra2.css">
    <title>AS Colombia</title>
</head>

<body>
    <div class="container mt-4" id="divContra">
        <form style="width: 320px;" class="mt-4 container" method="POST" action="/index/resetFinal/<%= token%>">
            <div class="text-center mt-3 mb-4">
                <h3 class="mb-4"><b>Recuperar Contraseña</b></h2>
            </div>
            <div class="form-group">
                <label>Nueva Contraseña</label>
                <div class="input-group" id="show_hide_password">
                    <input class="form-control" type="password" name="contrasena_Agricultor">
                    <div class="eye1">
                        <a href="#"><i class="fa fa-eye-slash" aria-hidden="true"></i></a>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label>Repetir Contraseña</label>
                <div class="input-group" id="show_hide_password2">
                    <input class="form-control" type="password" name="repetir_Contrasena">
                    <div class="eye1">
                        <a href="#"><i class="fa fa-eye-slash" aria-hidden="true"></i></a>
                    </div>
                </div>
            </div>
            <div class="text-center mt-4"><button class="btn btn-success">Confirmar</button></div>
            <br><br>
        </form>
    </div>
    <br>
</body>
